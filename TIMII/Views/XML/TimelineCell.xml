<UITableViewCell
    reuseIdentifier="timelineCell"
    width=""
    height="auto + 30"
    selectedBackgroundView.backgroundColor="white"
    backgroundColor="clear"
    style=""
    layer.borderColor=""
    >
    
    <UIView
        template="TimelineDayTemplate.xml"
        dayNumberText="{dayNumberText}"
        dayText="{dayText}"
        width="12%"
        height="auto"
        backgroundColor="clear"
    />
    
    <UIView
        template="TimelineDayContentTemplate.xml"
        width="88%"
        height="100%"
        backgroundColor="porcelainOpaque"
    />
    
</UITableViewCell>
