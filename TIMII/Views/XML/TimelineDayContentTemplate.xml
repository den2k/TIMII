<!-- TimelineDayContentTemplate.xml -->

<!-- This shows the member's per day content -->
<UIView
    left="previous.right"
    width="100%"
    height="auto"
    backgroundColor="">
    
    <macro name="dateTextColor" value="black" />
    <macro name="www" value="isWeekend" />
    
    <UIView
        width="100%"
        height="auto"
        top="0"
        left="0"
        backgroundColor="">
        
        <UILabel
            text="Today I went to work. Need to fight."
            width="100%"
            height="auto"
            top=""
            left="previous.left"
            textAlignment="left"
            textColor="dateTextColor"
            backgroundColor=""
            font="Avenir Next 14"
        />

    </UIView>
</UIView>

